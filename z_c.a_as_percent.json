{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/ergrko/ergrko.github.io/main/ONS_data_AA6H.json",
      "format": {"type": "json", "property": "years"}
    },
    "title": {
      "text": "Current Account Balance in the UK (% of GDP) ",
      "subtitle": "from 1946-2021, Source: ONS",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },
    "width": 200,
    "height": 180,
    "mark": {"type": "line", "color": "rgb(0,47,167"},
    "encoding": {
      "x": {
        "field": "date",
        "type": "temporal",
        "axis": {"grid": false},
        "title": null
      },
      "y": {
        "field": "value",
        "type": "quantitative",
        "axis": {"grid": false},
        "title": "% of GDP"
      }
    }
  }